    var string mensaje_error = ""
    
    // Validar per√≠odo
    if periodo_principal < 1 or periodo_principal > 500
        mensaje_error := "‚ùå Error: El per√≠odo debe estar entre 1 y 500"
        runtime.error(mensaje_error)
    
    // Validar fuente de datos
    if na(fuente_datos)
        mensaje_error := "‚ùå Error: Fuente de datos no v√°lida"
        runtime.error(mensaje_error)
    
    true

