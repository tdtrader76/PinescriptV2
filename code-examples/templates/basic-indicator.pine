// Funci√≥n para calcular diferentes tipos de medias m√≥viles
calcularMediaMovil(src, longitud, tipo) =>
    switch tipo
        "SMA" => ta.sma(src, longitud)
        "EMA" => ta.ema(src, longitud)
        "WMA" => ta.wma(src, longitud)
        "VWMA" => ta.vwma(src, longitud)
        => ta.sma(src, longitud)  // Valor por defecto

// Funci√≥n para detectar cruces alcistas
esCruceAlcista(serie1, serie2) =>
    ta.crossover(serie1, serie2)

// Funci√≥n para detectar cruces bajistas
esCruceBajista(serie1, serie2) =>
    ta.crossunder(serie1, serie2)

// Funci√≥n para verificar si una vela es alcista
esVelaAlcista() =>
    close > open

// Funci√≥n para verificar si una vela es bajista
esVelaBajista() =>
    close < open

